      "roles.edit",
      "roles.archive",
      "roles.restore"
    ],
    "rbac_visibility_rules": {
      "admin": "all by perms (usually wildcard or full set)",
      "pm": "only own projects + tasks of those projects (backend filters)",
      "fin": "projects where fin has tasks + own tasks (backend filters)",
      "all_users": "see only own tasks unless admin"
    }
  },

  "routes": {
    "login": "#/login",
    "main.index": "#/main",

    "settings.index": "#/settings",
    "settings.view": "#/settings/view?dict=:dict&archived=:0or1",
    "settings.new": "#/settings/new?dict=:dict",
    "settings.edit": "#/settings/edit?dict=:dict&id=:id",

    "roles.index": "#/roles",
    "roles.view": "#/roles/view?id=:id",
    "roles.new": "#/roles/new",
    "roles.edit": "#/roles/edit?id=:id",

    "users.index": "#/users",
    "users.view": "#/users/view?id=:id",
    "users.new": "#/users/new",
    "users.edit": "#/users/edit?id=:id",

    "clients.index": "#/clients",
    "clients.view": "#/clients/view?id=:id",
    "clients.new": "#/clients/new",
    "clients.edit": "#/clients/edit?id=:id",

    "course_catalog.index": "#/course_catalog",
    "course_catalog.view": "#/course_catalog/view?id=:id",
    "course_catalog.new": "#/course_catalog/new",
    "course_catalog.edit": "#/course_catalog/edit?id=:id",

    "courses.index": "#/courses",
    "courses.view": "#/courses/view?id=:id",
    "courses.new": "#/courses/new",
    "courses.edit": "#/courses/edit?id=:id",

    "projects.index": "#/projects",
    "projects.view": "#/projects/view?id=:id",
    "projects.new": "#/projects/new",
    "projects.edit": "#/projects/edit?id=:id",

    "tasks.index": "#/tasks",
    "tasks.view": "#/tasks/view?id=:id",
    "tasks.new": "#/tasks/new",
    "tasks.edit": "#/tasks/edit?id=:id"
  },

  "dicts": {
    "source": "settings/dict_items API",
    "dicts_list": [
      { "key": "cities", "title_i18n": "dict.cities" },
      { "key": "sources", "title_i18n": "dict.sources" },
      { "key": "spheres", "title_i18n": "dict.spheres" },
      { "key": "task_statuses", "title_i18n": "dict.task_statuses" },
      { "key": "project_statuses", "title_i18n": "dict.project_statuses" },
      { "key": "lead_statuses", "title_i18n": "dict.lead_statuses" },
      {
        "key": "project_types",
        "title_i18n": "dict.project_types",
        "extra_fields": ["final_type"],
        "final_type_enum": ["success", "cancel"]
      }
    ],
    "dict_item_common_fields": [
      "id",
      "dict_key",
      "name",
      "color",
      "sort",
      "active",
      "is_default",
      "is_deleted",
      "created_at",
      "updated_at"
    ],
    "dict_item_extra_fields": {
      "project_types": ["final_type"]
    }
  },

  "api": {
    "rules": {
      "credentials": "always include",
      "on_401": "redirect to #/login",
      "errors": "inline error or toast (not alert)"
    },
    "endpoints": {
      "auth": [
        { "method": "POST", "path": "/auth/login", "returns": "{ok} sets cookie" },
        { "method": "POST", "path": "/auth/logout", "returns": "{ok}" },
        { "method": "GET", "path": "/me", "returns": "{ok,user,perms}" }
      ],

      "settings": [
        { "method": "GET", "path": "/settings/dicts", "perm": "settings.index", "returns": "{ok,dicts:[{key,title_i18n}]}" },
        { "method": "GET", "path": "/settings/dict-items?dict=:key&include_deleted=:0or1", "perm": "settings.view", "returns": "{ok,items:[dict_item]}" },
        { "method": "GET", "path": "/settings/dict-items/:id", "perm": "settings.view", "returns": "{ok,item:dict_item}" },
        { "method": "POST", "path": "/settings/dict-items", "perm": "settings.create", "body": "{dict_key,name,color,sort,active,is_default,final_type?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/settings/dict-items/:id", "perm": "settings.edit", "body": "{name?,color?,sort?,active?,is_default?,is_deleted?,final_type?}", "returns": "{ok,item}" }
      ],

      "roles": [
        { "method": "GET", "path": "/roles", "perm": "roles.index", "returns": "{ok,items:[role]}" },
        { "method": "GET", "path": "/roles/:id", "perm": "roles.view", "returns": "{ok,item:role}" },
        { "method": "POST", "path": "/roles", "perm": "roles.create", "body": "{role_key,name}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/roles/:id", "perm": "roles.edit", "body": "{role_key?,name?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/roles/:id/archive", "perm": "roles.archive", "returns": "{ok}" },
        { "method": "PATCH", "path": "/roles/:id/restore", "perm": "roles.restore", "returns": "{ok}" },
        { "method": "GET", "path": "/roles/:id/permissions", "perm": "roles.view", "returns": "{ok,perms:[perm_key]}" },
        { "method": "PATCH", "path": "/roles/:id/permissions", "perm": "roles.edit", "body": "{perms:[perm_key]}", "returns": "{ok,perms:[perm_key]}" }
      ],

      "users": [
        { "method": "GET", "path": "/users", "perm": "users.index", "returns": "{ok,items:[user]}" },
        { "method": "GET", "path": "/users/:id", "perm": "users.view", "returns": "{ok,item:user}" },
        { "method": "POST", "path": "/users", "perm": "users.create", "body": "{login,password,full_name,phone,role_id,lang?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/users/:id", "perm": "users.edit", "body": "{login?,password?,full_name?,phone?,role_id?,lang?,active?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/users/:id/archive", "perm": "users.archive", "returns": "{ok}" },
        { "method": "PATCH", "path": "/users/:id/restore", "perm": "users.restore", "returns": "{ok}" }
      ],

      "clients": [
        { "method": "GET", "path": "/clients", "perm": "clients.index", "returns": "{ok,items:[client]}" },
        { "method": "GET", "path": "/clients/:id", "perm": "clients.view", "returns": "{ok,item:client,projects?:[],course_leads?:[]}" },
        { "method": "POST", "path": "/clients", "perm": "clients.create", "body": "{company_name,full_name?,phone1?,phone2?,city_id?,source_id?,sphere?,comment?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/clients/:id", "perm": "clients.edit", "body": "{company_name?,full_name?,phone1?,phone2?,city_id?,source_id?,sphere?,comment?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/clients/:id/archive", "perm": "clients.archive", "returns": "{ok}" },
        { "method": "PATCH", "path": "/clients/:id/restore", "perm": "clients.restore", "returns": "{ok}" }
      ],

      "course_catalog": [
        { "method": "GET", "path": "/course_catalog", "perm": "course_catalog.index", "returns": "{ok,items:[course_catalog]}" },
        { "method": "GET", "path": "/course_catalog/:id", "perm": "course_catalog.view", "returns": "{ok,item:course_catalog}" },
        { "method": "POST", "path": "/course_catalog", "perm": "course_catalog.create", "body": "{name,price,currency,active}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/course_catalog/:id", "perm": "course_catalog.edit", "body": "{name?,price?,currency?,active?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/course_catalog/:id/archive", "perm": "course_catalog.archive", "returns": "{ok}" },
        { "method": "PATCH", "path": "/course_catalog/:id/restore", "perm": "course_catalog.restore", "returns": "{ok}" }
      ],

      "courses": [
        { "method": "GET", "path": "/courses", "perm": "courses.index", "returns": "{ok,items:[course]}" },
        { "method": "GET", "path": "/courses/:id", "perm": "courses.view", "returns": "{ok,item:course,enrollments?:[]}" },
        { "method": "POST", "path": "/courses", "perm": "courses.create", "body": "{name,catalog_id?,start_at?,end_at?,active?,comment?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/courses/:id", "perm": "courses.edit", "body": "{name?,catalog_id?,start_at?,end_at?,active?,comment?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/courses/:id/archive", "perm": "courses.archive", "returns": "{ok}" },
        { "method": "PATCH", "path": "/courses/:id/restore", "perm": "courses.restore", "returns": "{ok}" }
      ],

      "projects": [
        { "method": "GET", "path": "/projects", "perm": "projects.index", "returns": "{ok,items:[project]}" },
        { "method": "GET", "path": "/projects/:id", "perm": "projects.view", "returns": "{ok,item:project,tasks?:[]}" },
        { "method": "POST", "path": "/projects", "perm": "projects.create", "body": "{name,client_id,status_id?,type_id?,owner_user_id?,start_at?,end_at?,comment?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/projects/:id", "perm": "projects.edit", "body": "{name?,client_id?,status_id?,type_id?,owner_user_id?,start_at?,end_at?,comment?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/projects/:id/archive", "perm": "projects.archive", "returns": "{ok}" },
        { "method": "PATCH", "path": "/projects/:id/restore", "perm": "projects.restore", "returns": "{ok}" }
      ],

      "tasks": [
        { "method": "GET", "path": "/tasks", "perm": "tasks.index", "returns": "{ok,items:[task]}", "notes": "admin can filter by user; others backend-filtered" },
        { "method": "GET", "path": "/tasks/:id", "perm": "tasks.view", "returns": "{ok,item:task}" },
        { "method": "POST", "path": "/tasks", "perm": "tasks.create", "body": "{title,description?,project_id,status_id,assignee_user_id?,due_at?,priority?,comment?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/tasks/:id", "perm": "tasks.edit", "body": "{title?,description?,project_id?,status_id?,assignee_user_id?,due_at?,priority?,comment?}", "returns": "{ok,item}" },
        { "method": "PATCH", "path": "/tasks/:id/archive", "perm": "tasks.archive", "returns": "{ok}" },
        { "method": "PATCH", "path": "/tasks/:id/restore", "perm": "tasks.restore", "returns": "{ok}" }
      ]
    }
  },

  "requisites": {
    "settings": {
      "entity": "dict_items",
      "soft_delete": { "flag": "is_deleted", "archive": true, "restore": true },
      "fields_common": [
        "id",
        "dict_key",
        "name",
        "color",
        "sort",
        "active",
        "is_default",
        "is_deleted",
        "created_at",
        "updated_at"
      ],
      "fields_by_dict": {
        "project_types": ["final_type"]
      },
      "ui": {
        "index": { "left_dict_list": true, "right_items_list": true },
        "list_columns": ["name", "color", "sort", "active"],
        "form_fields": ["name", "color", "sort", "active", "is_default", "final_type_if_project_types"],
        "archived_toggle": true
      }
    },

    "roles": {
      "entity": "roles",
      "soft_delete": { "flag": "is_deleted", "archive": true, "restore": true },
      "fields": ["id", "role_key", "name", "is_deleted", "created_at", "updated_at"],
      "permissions_model": {
        "storage": "role_permissions (role_id -> permission_id)",
        "permission_key_source": "permissions.key",
        "ui_matrix": "modules x actions (checkboxes)",
        "actions": ["index", "view", "create", "edit", "archive", "restore"]
      },
      "ui": {
        "index_columns": ["role_key", "name"],
        "form_fields": ["role_key", "name"],
        "permissions_editor": true
      }
    },

    "users": {
      "entity": "users",
      "soft_delete": { "flag": "is_deleted_or_active", "archive": true, "restore": true },
      "fields": ["id", "login", "full_name", "phone", "role_id", "lang", "active", "is_deleted", "created_at", "updated_at"],
      "relations": {
        "role": { "from": "roles", "key": "role_id", "label_fields": ["name", "role_key"] }
      },
      "ui": {
        "index_columns": ["login", "full_name", "phone", "role_id", "active"],
        "form_fields": ["login", "password_optional_edit", "full_name", "phone", "role_id", "lang", "active"],
        "archive_restore": true
      }
    },

    "clients": {
      "entity": "clients",
      "soft_delete": { "flag": "is_deleted", "archive": true, "restore": true },
      "dict_dependencies": {
        "city_id": "cities",
        "source_id": "sources"
      },
      "fields": [
        "id",
        "code",
        "company_name",
        "full_name",
        "phone1",
        "phone2",
        "city_id",
        "source_id",
        "sphere",
        "comment",
        "is_deleted",
        "deleted_at",
        "deleted_by",
        "created_at",
        "updated_at"
      ],
      "ui": {
        "index_columns": ["code", "company_name", "full_name", "phone1", "city_id", "source_id"],
        "form_fields": ["company_name", "full_name", "phone1", "phone2", "city_id", "source_id", "sphere", "comment"],
        "view_sections": [
          "client_data",
          "client_projects",
          "client_course_leads"
        ]
      }
    },

    "course_catalog": {
      "entity": "courses_catalog",
      "soft_delete": { "flag": "is_deleted_or_active", "archive": true, "restore": true },
      "fields": ["id", "name", "price", "currency", "active", "is_deleted", "created_at", "updated_at"],
      "ui": {
        "index_columns": ["name", "price", "currency", "active"],
        "form_fields": ["name", "price", "currency", "active"],
        "view_sections": ["catalog_item"]
      }
    },

    "courses": {
      "entity": "courses",
      "soft_delete": { "flag": "is_deleted_or_active", "archive": true, "restore": true },
      "relations": {
        "catalog": { "from": "courses_catalog", "key": "catalog_id", "label_fields": ["name"] }
      },
      "fields": [
        "id",
        "name",
        "catalog_id",
        "start_at",
        "end_at",
        "active",
        "comment",
        "is_deleted",
        "created_at",
        "updated_at"
      ],
      "ui": {
        "index_mode": "kanban",
        "kanban_group_by": "status_like (if backend provides) or active",
        "index_columns": ["name", "catalog_id", "start_at", "active"],
        "form_fields": ["name", "catalog_id", "start_at", "end_at", "active", "comment"],
        "view_sections": ["course_data", "course_enrollments_if_any"]
      }
    },

    "projects": {
      "entity": "projects",
      "soft_delete": { "flag": "is_deleted", "archive": true, "restore": true },
      "dict_dependencies": {
        "status_id": "project_statuses",
        "type_id": "project_types"
      },
      "relations": {
        "client": { "from": "clients", "key": "client_id", "label_fields": ["company_name"] },
        "owner": { "from": "users", "key": "owner_user_id", "label_fields": ["full_name", "login"] }
      },
      "fields": [
        "id",
        "name",
        "client_id",
        "status_id",
        "type_id",
        "owner_user_id",
        "start_at",
        "end_at",
        "comment",
        "is_deleted",
        "deleted_at",
        "deleted_by",
        "created_at",
        "updated_at"
      ],
      "ui": {
        "index_mode": "kanban",
        "kanban_group_by": "status_id",
        "index_columns": ["name", "client_id", "status_id", "type_id", "owner_user_id"],
        "form_fields": ["name", "client_id", "status_id", "type_id", "owner_user_id", "start_at", "end_at", "comment"],
        "view_sections": ["project_data", "project_tasks"]

{
  "id": "GSOFT2-REGISTER-v3",
  "tz_id": "GSOFT2-FRONT-JARVIS-TZ-v1",
  "base_id": "GSOFT-BASE-v1:d91e2932409a",
  "timezone": "Asia/Tashkent",

  "non_breakable_rules": {
    "frontend_structure": "index.html + view/{module}/{index,view,new,edit}.html + controllers/{core + modules}.js",
    "pages_not_modals": true,
    "languages": ["ru", "uz", "en"],
    "single_i18n_controller": "controllers/i18n.js",
    "rbac_frontend": "UI hides forbidden, backend is source of truth",
    "soft_delete": "archive/restore; no hard delete from UI",
    "dicts_from_api_only": "all select lists come from settings/dict_items via API",
    "no_new_tables_or_values": true,
    "field_names": "frontend uses backend field names; no renaming"
  },

  "constants": {
    "API_BASE": "https://api.ofis.gekto.uz",
    "DEFAULT_ROUTE": "#/main",
    "LANG_ORDER": ["ru", "uz", "en"],
    "AUTH_COOKIE": "gsoft_session",
    "SESSION_RULE": "one active session per user; expires 23:59:59 Tashkent",
    "CORS_ALLOW": ["https://ofis.gekto.uz", "http://localhost:5173", "http://localhost:3000"]
  },

  "frontend_structure": {
    "root_files": ["index.html"],
    "views_dir": "view/",
    "controllers_dir": "controllers/",
    "views": {
      "login": "view/login.html",
      "modules": {
        "main": ["index.html"],
        "tasks": ["index.html", "view.html", "new.html", "edit.html"],
        "projects": ["index.html", "view.html", "new.html", "edit.html"],
        "courses": ["index.html", "view.html", "new.html", "edit.html"],
        "course_catalog": ["index.html", "view.html", "new.html", "edit.html"],
        "clients": ["index.html", "view.html", "new.html", "edit.html"],
        "settings": ["index.html", "view.html", "new.html", "edit.html"],
        "users": ["index.html", "view.html", "new.html", "edit.html"],
        "roles": ["index.html", "view.html", "new.html", "edit.html"]
      }
    },
    "controllers": {
      "core": [
        "controllers/app.js",
        "controllers/router.js",
        "controllers/api.js",
        "controllers/i18n.js",
        "controllers/rbac.js",
        "controllers/viewLoader.js",
        "controllers/moduleFactory.js",
        "controllers/appShell.js"
      ],
      "modules": [
        "controllers/main.js",
        "controllers/tasks.js",
        "controllers/projects.js",
        "controllers/courses.js",
        "controllers/course_catalog.js",
        "controllers/clients.js",
        "controllers/settings.js",
        "controllers/users.js",
        "controllers/roles.js"
      ]
    },
    "controller_contract": {
      "load": "load(ctx) loads template + data via API",
      "calc": "calc(ctx, data) builds view-model (grouping/kanban/counters)",
      "render": "render(ctx, vm) returns HTML string",
      "mount": "mount(ctx, vm) binds listeners after HTML inserted"
    }
  },

  "router": {
    "default_route": "#/main",
    "login_route": "#/login",
    "auth_guard": {
      "startup_check": { "method": "GET", "path": "/me" },
      "on_401": "redirect #/login",
      "on_ok": "window.APP.user + window.APP.perms; i18n.syncFromUser(user)"
    },
    "hash_rules": {
      "basePath_active_menu": "take first segment: #/tasks/view -> /tasks",
      "query_params": "standard ?a=b&c=d"
    }
  },

  "shell": {
    "sidebar": {
      "menu_visibility_rule": "show module if hasPerm('<module>.index') OR perms includes '*'",
      "sections": ["main", "tasks", "projects", "courses", "course_catalog", "clients", "settings", "users", "roles"],
      "active_rule": "highlight by basePath",
      "icons_source": "icons/*.svg (mask + currentColor)"
    },
    "header": {
      "left": "module title via i18n",
      "right": ["user_name", "user_role", "language_switch", "logout"]
    },
    "main": {
      "outlet_id": "routeOutlet",
      "container": "card panels"
    }
  },

  "ui_jarvis": {
    "theme_vars": [
      "--bg",
      "--panel",
      "--text",
      "--muted",
      "--accent",
      "--accent2",
      "--danger",
      "--success",
      "--warning",
      "--stroke",
      "--stroke2"
    ],
    "rules": {
      "no_colors_in_js": true,
      "css_vars_only": true,
      "glass_panels": true,
      "thin_grid_tables": true,
      "animation_ms": [150, 250]
    },
    "base_classes": {
      "card": ".card",
      "btn": ".btn / .btn.ghost / .btn.sm",
      "input": ".input",
      "table": ".techTable"
    }
  },

  "icons": {
    "root": "icons/",
    "files": [
      "asosiy.svg",
      "clients.svg",
      "cources.svg",
      "catalog.svg",
      "delete.svg",
      "edit.svg",
      "projects.svg",
      "settings.svg",
      "tasks.svg",
      "users.svg",
      "roles.svg"
    ],
    "nav_map": {
      "main": "asosiy.svg",
      "tasks": "tasks.svg",
      "projects": "projects.svg",
      "courses": "cources.svg",
      "course_catalog": "catalog.svg",
      "clients": "clients.svg",
      "settings": "settings.svg",
      "users": "users.svg",
      "roles": "roles.svg"
    },
    "actions": {
      "edit": "edit.svg",
      "archive": "delete.svg"
    }
  },

  "permissions": {
    "format": "module.action",
    "actions": ["index", "view", "create", "edit", "archive", "restore"],
    "wildcard": "*",
    "menu_visibility": "module visible if <module>.index allowed",
    "modules": [
      "main",
      "tasks",
      "projects",
      "courses",
      "course_catalog",
      "clients",
      "settings",
      "users",
      "roles"
    ],
    "generated_keys": [
      "main.index",

      "tasks.index",
      "tasks.view",
      "tasks.create",
      "tasks.edit",
      "tasks.archive",
      "tasks.restore",

      "projects.index",
      "projects.view",
      "projects.create",
      "projects.edit",
      "projects.archive",
      "projects.restore",

      "courses.index",
      "courses.view",
      "courses.create",
      "courses.edit",
      "courses.archive",
      "courses.restore",

      "course_catalog.index",
      "course_catalog.view",
      "course_catalog.create",
      "course_catalog.edit",
      "course_catalog.archive",
      "course_catalog.restore",

      "clients.index",
      "clients.view",
      "clients.create",
      "clients.edit",
      "clients.archive",
      "clients.restore",

      "settings.index",
      "settings.view",
      "settings.create",
      "settings.edit",
      "settings.archive",
      "settings.restore",

      "users.index",
      "users.view",
